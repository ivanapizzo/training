{
  "name": "halloween",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "devNode": "nodemon --watch dist --ext js --exec node dist/index.js",
    "dev1": "tsc --watch --preserveWatchOutput & nodemon --watch dist --ext js --exec node dist/index.js",
    "dev2": "nodemon --watch src --ext ts --exec tsx src/index.ts",
    "dev3": "tsx watch src/index.ts",
    "dev4": "nodemon --watch src/index.ts --exec echo src/index.ts > ivana.txt",

    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run"

  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "chalk": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
